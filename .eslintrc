{
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      2,
      2, {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-reserved-keys": 0,
    "no-wrap-func": 0,
    "spaced-line-comment": 0,
    "no-duplicate-case": 0,
    "no-else-return": 0,
    "react/sort-comp": [1, {}],
    "no-undef": 1,
    "object-curly-spacing": [0],
    "react/no-did-mount-set-state": [0]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "_": true,
    "__ENV__": true,
  },
  "extends": ["airbnb", "airplus"],
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}
